<h3>Clock events</h3>

<ul class="collection">
    <%= for clock <- @clocks do %>
            <li class="collection-item">
            Check in:
            <%= clock.check_in %>
            Check out: 
            <%= clock.check_out%>
            <div class="right">
            Owned by:  
            <%= clock.employee.name%> 
            </div>
            </li>
    <%end%>
</ul>